{
  "manifest_version": 3,
  "name": "Optizchat - AI Made Easy",
  "version": "1.0.0",
  "description": "A powerful AI assistant that makes working with AI models easy",
  "permissions": ["storage", "activeTab"],
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}